package entities;

import java.util.Date;

public class Prestamo {
	private String idPrestamo;
	private Date fecPres;
	private Date fecDev;
	private String estado;
	private String idLibro;
	private String idAlumno;
	private String titulo;
	private String alumno;
	public Prestamo() {

	}
	public Prestamo(String idPrestamo, Date fecPres, Date fecDev, String estado, String idLibro, String idAlumno) {
		//super();
		this.idPrestamo = idPrestamo;
		this.fecPres = fecPres;
		this.fecDev = fecDev;
		this.estado = estado;
		this.idLibro = idLibro;
		this.idAlumno = idAlumno;
	}
	public Prestamo(String idPrestamo, Date fecPres, Date fecDev, String estado, String titulo, String alumno, String sw) {
		//super();
		this.idPrestamo = idPrestamo;
		this.fecPres = fecPres;
		this.fecDev = fecDev;
		this.estado = estado;
		this.titulo = titulo;
		this.alumno = alumno;
	}
	
	
	
